{
  "name": "nice-gadgets-product_catalog",
  "homepage": "https://OAO-A.github.io/product_cards",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@types/styled-components": "4.0.3",
    "babel-plugin-styled-components": "^1.10.0",
    "classnames": "^2.3.2",
    "framer-motion": "^7.6.4",
    "gh-pages": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-grid-system": "^8.1.6",
    "react-router-dom": "^6.4.2",
    "react-scripts": "^5.0.1",
    "sass": "^1.55.0",
    "styled-components": "4.0.3",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "fix-style": "npm run format && npm run lint -- --fix",
    "prepare": "cd .. && husky install ./product_catalog-FrontEnd/.husky",
    "format": "npx prettier --write . --ignore-path .gitignore",
    "lint": "eslint ."
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@mate-academy/eslint-config-react-typescript": "^1.0.11",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.10",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "stylelint-config-recommended-scss": "^8.0.0",
    "typescript": "^4.8.4"
  },
  "lint-staged": {
    "*.ts": [
      "npm run fix-style",
      "git add ."
    ]
  }
}
